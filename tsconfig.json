{
    "compilerOptions": {
        "target": "ES6",
        "module": "node16",
        "lib": ["es2017", "es7", "es6", "dom"],
        "declaration": true,
        "strict": true,
        "esModuleInterop": true,
        "skipLibCheck": true,
        "forceConsistentCasingInFileNames": true,
        "outDir": "dist",
        "moduleResolution": "nodenext"
    },
    "exclude": ["node_modules", "dist"]
}
